		"nextra": "^4.3.0",
		"nextra-theme-docs": "^4.3.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1"
	},
	"devDependencies": {
		"@types/node": "^24.3.0",
		"@types/react": "^19.1.10",
		"pagefind": "^1.3.0",
		"tsx": "^4.19.3",
		"typescript": "^5.9.2"
	},
	"license": "MIT"
}
